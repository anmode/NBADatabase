
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>Import Student data from CSV File to mongoDB</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
  </head>
  <body > <h1 align="center"> STUDENT DATA IMPORTING FROM CSV/XL FILES</h1> 
    <h2>  <a href="http://localhost:5500/studentpage"><button class="btn btn-success">go to student page</button></a></h2>
    <img src="<% require('/image/co.png') %>" alt="" class="logo">
    <form action="/studentINFO/importcsv" method="post" enctype="multipart/form-data">
      <h2 align="center"> STUDENT DATA IMPORTING FROM CSV FILES</h2> 
      <div class="mb-3">
        <input type="file" class="form-control" name="csvFile" />
      </div>
      <button type="submit" class="btn btn-success">Upload CSV</button>
      <div class="mb-4"></div>
     </form>
     <h2 align="center"> STUDENT DATA IMPORTING FROM XL FILES</h2> 
     <div class="mb-4"></div>
     <form action="/studentINFO/importxl" method="post" enctype="multipart/form-data">
       
         <input type="file" class="form-control" name="xlFile" />
       </div>
     <button type="submit" class="btn btn-success">Upload XL</button>
   </form>

      <%if(data){%>
      <div class="container mt-4">
        <table class="table">
          <thead>
            <tr>
              <th>name</th>
              <th>enrollment</th>
              <th>facultyNum</th>
              <th>father</th>
              <th>mother</th>
              <th>Permanent Address</th>
              <th>Corresponding Address</th>
              <th>Phone Number</th>
              <th>Email</th>
              <th>fatherNum</th>
              <th>hall</th>
              <th>dob</th>
              <th>nationality</th>
            </tr>
          </thead>
          <tbody>
            <%for(var i=0; i < data.length; i++){%>
            
              <tr>
              
              <td><%= data[i].name%></td>
              <td><%= data[i].enrollment%></td>
              <td><%= data[i].facultyNum%></td>
              <td><%= data[i].father%></td>
              <td><%= data[i].mother%></td>
              <td><%= data[i].addrPermanent%></td>
              <td><%= data[i].addrCorresponding%></td>
              <td><%= data[i].phoneNum%></td>
              <td><%= data[i].Email%></td>
              <td><%= data[i].fatherNum%></td>
              <td><%= data[i].hall%></td>
              <td><%= data[i].dob%></td>
              <td><%= data[i].nationality%></td>
            </tr>
            <%}%>
          </tbody>
        </table>
      </div>
      <%}%>

      <form action="/facultyINFO/importcsv" method="post" enctype="multipart/form-data">
        <h2 align="center"> STUDENT DATA IMPORTING FROM CSV FILES</h2> 
        <div class="mb-3">
          <input type="file" class="form-control" name="csvFile" />
        </div>
        <button type="submit" class="btn btn-success">Upload CSV</button>
        <div class="mb-4"></div>
       </form>
       <h2 align="center"> STUDENT DATA IMPORTING FROM XL FILES</h2> 
       <div class="mb-4"></div>
       <form action="/facultyINFO/importxl" method="post" enctype="multipart/form-data">
         
           <input type="file" class="form-control" name="xlFile" />
         </div>
       <button type="submit" class="btn btn-success">Upload XL</button>
     </form>
     <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-kenU1KFdBIe4zVF0s0G1M5b4hcpxyD9F7jL+jjXkk+Q2h455rYXK/7HAuoJl+0I4" crossorigin="anonymous"></script>
  </body>
</html>